  <div class='container'>
    <h3>配送先編集</h3>
    <div class="mx-auto">
      <%= form_with model: @Address, local:true do |f| %>
        <%= f.label '郵便番号(ハイフンなし)' %>
        <%= f.text_field :postal_code %><br>
        <%= f.label '住所'  %>
        <%= f.text_field :address %><br>
        <%= f.label '宛名'%>
        <%= f.text_field :name %>
        <%= f.submit '変更内容を保存', class: 'btn btn-sm btn-success' %>
      <% end %>
    </div>
  </div>
