  <div class='containder'>
    <%= form_with model: @Address, local:true do |f| %>
    <%= f.label '郵便番号(ハイフンなし)' %>
    <%= f.text_field :postal_code %>
    <%= f.label '住所' %>
    <%= f.text_field :address %>
    <%= f.label '宛名'%>
    <%= f.text_field :name %>
    <%= f.submit '新規登録', class: 'btn btn-sm btn-success' %>
    <% end %>
  </div>
