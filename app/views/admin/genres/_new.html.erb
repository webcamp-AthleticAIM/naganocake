
  <%= form_with model: [:admin, genre], local:true do |f| %>
  <div class="row">
    <div class="col mt-5">
      <%= f.label "ジャンル名" %>
    </div>
    <div class="col mt-5">
      <%= f.text_field :name, placeholder: "ジャンル名", class: "form-control" %>
    </div>
    <div class="col mt-5">
      <%= f.submit "新規登録", class: "btn btn-success" %>
    </div>
  </div>
  <% end %>

